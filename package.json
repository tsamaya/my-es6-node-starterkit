{
  "name": "my-es6-node-starterkit",
  "version": "2.2.0",
  "description": "es6 node starter kit",
  "homepage": "https://github.com/tsamaya/my-es6-node-starterkit",
  "keywords": [
    "node",
    "module",
    "es6",
    "webpack",
    "jest",
    "babel",
    "prettier",
    "eslint",
    "editorconfig"
  ],
  "bugs": "https://github.com/tsamaya/my-es6-node-starterkit/issues",
  "license": "MIT",
  "author": "Arnaud Ferrand",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsamaya/my-es6-node-starterkit.git"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "webpack -d",
    "codecov": "codecov",
    "coverage": "npm run test && npm run codecov",
    "lint": "eslint .",
    "prebuild": "npm test",
    "precommit": "lint-staged",
    "pretest": "npm run lint",
    "test": "jest --env=node --coverage",
    "test:watch": "npm test -- --watch",
    "webpack": "webpack -d --watch"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "codecov": "^3.0.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.20.0",
    "eslint-plugin-prettier": "^2.6.2",
    "husky": "^0.14.3",
    "jest": "^22.4.4",
    "lint-staged": "^6.1.1",
    "prettier": "^1.14.2",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0"
  }
}
