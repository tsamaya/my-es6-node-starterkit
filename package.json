{
  "name": "my-es6-node-starterkit",
  "version": "3.0.0",
  "description": "es6 node starter kit",
  "homepage": "https://github.com/tsamaya/my-es6-node-starterkit",
  "keywords": [
    "node",
    "module",
    "es6",
    "webpack",
    "jest",
    "babel",
    "prettier",
    "eslint",
    "editorconfig"
  ],
  "bugs": "https://github.com/tsamaya/my-es6-node-starterkit/issues",
  "license": "MIT",
  "author": "Arnaud Ferrand",
  "main": "./dist/es6-node-starterkit.js",
  "browser": "./dist/es6-starterkit.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tsamaya/my-es6-node-starterkit.git"
  },
  "lint-staged": {
    "*.{js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "build": "webpack --mode production",
    "codecov": "codecov",
    "coverage": "npm run test && npm run codecov",
    "lint": "eslint .",
    "prebuild": "npm test",
    "precommit": "lint-staged",
    "pretest": "npm run lint",
    "test": "jest --env=node --coverage",
    "test:watch": "npm test -- --watch",
    "webpack": "webpack --mode development --watch"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.0",
    "@babel/preset-env": "^7.20.2",
    "codecov": "^3.8.3",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {}
}
